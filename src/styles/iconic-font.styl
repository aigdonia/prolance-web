@charset "UTF-8";

@font-face {
  font-family: "prolance";
  src:url("fonts/prolance.eot");
  src:url("fonts/prolance.eot?#iefix") format("embedded-opentype"),
    url("fonts/prolance.woff") format("woff"),
    url("fonts/prolance.ttf") format("truetype"),
    url("fonts/prolance.svg#prolance") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "prolance" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "prolance" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-prolance:before {
  content: "\61";
}
.icon-slack:before {
  content: "\62";
}
.icon-magic:before {
  content: "\63";
}
.icon-help:before {
  content: "\64";
}
.icon-facebook:before {
  content: "\65";
}
.icon-hourglass:before {
  content: "\66";
}
.icon-cubes:before {
  content: "\67";
}
